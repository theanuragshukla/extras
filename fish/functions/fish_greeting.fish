function fish_greeting
starship init fish | source  
alias ls='exa --icons --no-user --group-directories-first  --time-style long-iso'
alias ll='exa -l --icons --no-user --group-directories-first  --time-style long-iso'
alias la='exa -l -a --icons --no-user --group-directories-first  --time-style long-iso'
end
